{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "React Native / Expo",
  "extends": "./base.json",
  "compilerOptions": {
    "target": "ESNext",
    "lib": ["ES2022"],
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "strict": true,
    "noEmit": true,
    "allowJs": true,
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "skipLibCheck": true
  }
}
