{
  "expo": {
    "name": "BabyNest",
    "slug": "babynest",
    "version": "0.1.0",
    "orientation": "portrait",
    "scheme": "babynest",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "resizeMode": "contain",
      "backgroundColor": "#c026d3"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.babynest.app"
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#c026d3"
      },
      "package": "com.babynest.app"
    },
    "web": {
      "bundler": "metro",
      "output": "single"
    },
    "plugins": ["expo-router"],
    "experiments": {
      "typedRoutes": true
    }
  }
}
